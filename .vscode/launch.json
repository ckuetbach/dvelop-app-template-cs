{
    // cf. https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SelfHosted/HostApplication/bin/Debug/netcoreapp3.1/HostApplication.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "SYSTEMBASEURI": "http://localhost:5000",
                "SIGNATURE_SECRET": "",
                "ASSET_BASE_PATH__": "https://s3-eu-central-1.amazonaws.com/assets.acme-apptemplatecs.hackathon.service.d-velop.cloud/d41d8cd98f00b204e9800998ecf8427e",
                "RELATIVE_ASSET_DIR": "${workspaceFolder}\\Remote\\wwwroot"
              }
        }

    ]
}